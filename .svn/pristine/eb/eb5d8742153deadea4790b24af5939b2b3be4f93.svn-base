.slider-pager .slider-pager-page{
        color: #808080;
        opacity: 0.4;
}
.slider-pager .slider-pager-page.active{
        opacity: 0.9;
}
.slider-pager {
    position: absolute;
    bottom: 12px;
    z-index: 1;
    width: 100%;
    height: 15px;
    text-align: center;
}
.tishi{
   
    font-size: 20px;
    color: #ec5f32;
    display: inline-block;
    margin-bottom: -4px;
    position: absolute;
}
.PositionStart{
    position: absolute;
    left: -270px;
    transition: 0s linear;
}
.PositionEnd{
    position: absolute;
    left: 100%;
    transition: 6s linear;
}
.PositionState{
    position: absolute;
    left: 15px;
    transition: 6s linear;
}
